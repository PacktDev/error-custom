{
  "name": "@packt/error-custom",
  "version": "0.1.1",
  "description": "Extends the JavaScript Error object with custom properties.",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:packt-internal/error-custom.git"
  },
  "main": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "npm run lint && npm run unit && npm run coverage",
    "lint": "eslint ./src",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage --statements 95 --functions 95 --branches 95 --lines 95",
    "unit": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover --include-all-sources --root ./src/lib ./node_modules/mocha/bin/_mocha -- ./tests/*.spec.js",
    "build": "webpack --mode production",
    "prepublishOnly": "npm run test && npm run build"
  },
  "engines": {
    "node": ">=8.10"
  },
  "author": {
    "name": "Packt Publishing Ltd",
    "url": "https://www.packtpub.com/"
  },
  "contributors": [
    {
      "name": "Marius Lazar",
      "email": "mariuslazar93@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/packt-internal/error-custom#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-istanbul": "0.12.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "istanbul": "0.4.5",
    "mocha": "5.0.5",
    "sinon": "6.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.2"
  },
  "dependencies": {
    "debug": "3.1.0",
    "joi": "13.4.0"
  }
}
